{
  "name": "edwin-luijten/houston",
  "description": "Error tracking",
  "license": "MIT",
  "authors": [
    {
      "name": "Edwin Luijten",
      "email": "edwin.luijten@live.nl"
    }
  ],
  "keywords": [
    "Houston we've got a problem"
  ],
  "require": {
    "php": ">=5.6.0",
    "psr/log": "^1.0",
    "monolog/monolog": "^1.21"
  },
  "require-dev": {
    "phpunit/phpunit": "~4.0",
    "mockery/mockery": "~0.9",
    "squizlabs/php_codesniffer": "~2.0"
  },
  "autoload": {
    "psr-4": {
      "EdwinLuijten\\Houston\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "EdwinLuijten\\Houston\\Tests\\": "tests/src/"
    }
  }
}
